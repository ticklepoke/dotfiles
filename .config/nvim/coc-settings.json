{
  "coc.preferences.colorSupport": "true",
  "coc.preferences.formatOnSaveFiletypes": [
    "css",
    "scss",
    "markdown",
    "javascript",
    "html",
    "graphql",
    "typescript",
    "json",
    "typescriptreact",
    "javascriptreact",
    "typescript.tsx",
    "go",
    "json",
    "rust"
  ],
  "suggest.noselect": false,
  "html.autoClosingTags": true,
  "pairs.enableCharacters": ["(", "[", "{", "'", "\"", "`"],
  "eslint.format.enable": true,
  "eslint.packageManager": "yarn",
  "prettier.eslintIntegration": true,
  "markdownlint.config": {
    "line-length": false
  },
  "suggest.completionItemKindLabels": {
    "function": "\uf794 function",
    "method": "\uf6a6 method",
    "variable": "\uf71b variable",
    "constant": "\uf8ff constant",
    "struct": "\ufb44 struct",
    "class": "\uf0e8 class",
    "interface": "\ufa52 interface",
    "text": "\ue612 text",
    "enum": "\uf435 enum",
    "enumMember": "\uf02b enumMember",
    "module": "\uf668 module",
    "color": "\ue22b",
    "property": "\ufab6 property",
    "field": "\uf93d field",
    "unit": "\uf475",
    "file": "\uf471 file",
    "value": "\uf8a3",
    "event": "\ufacd",
    "folder": "\uf115 folder",
    "keyword": "\uf893 keyword",
    "snippet": "\uf64d",
    "operator": "\uf915",
    "reference": "\uf87a reference",
    "typeParameter": "\uf278 typeparameter",
    "default": "\uf29c default"
  }
}
